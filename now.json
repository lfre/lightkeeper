{
  "version": 2,
  "name": "lightkeeper",
  "alias": [
    "lightkeeper.alfre.do"
  ],
  "env": {
    "APP_ID": "@app_id",
    "APP_NAME": "Lightkeeper",
    "NODE_ENV": "production",
    "PRIVATE_KEY": "@private_key",
    "WEBHOOK_SECRET": "@webhook_secret"
  },
  "builds": [
    {
      "src": "now.js",
      "use": "@now/node",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/*",
      "dest": "/now.js"
    }
  ]
}
